#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/harun.karahan/Mendix/native-widgets/node_modules/.pnpm/react-native@0.77.3_@babel+core@7.28.0_@babel+preset-env@7.28.0_@babel+core@7.28.0__@re_c4b2b877ff76cdef11524e8484232852/node_modules/react-native/node_modules:/Users/harun.karahan/Mendix/native-widgets/node_modules/.pnpm/react-native@0.77.3_@babel+core@7.28.0_@babel+preset-env@7.28.0_@babel+core@7.28.0__@re_c4b2b877ff76cdef11524e8484232852/node_modules:/Users/harun.karahan/Mendix/native-widgets/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/harun.karahan/Mendix/native-widgets/node_modules/.pnpm/react-native@0.77.3_@babel+core@7.28.0_@babel+preset-env@7.28.0_@babel+core@7.28.0__@re_c4b2b877ff76cdef11524e8484232852/node_modules/react-native/node_modules:/Users/harun.karahan/Mendix/native-widgets/node_modules/.pnpm/react-native@0.77.3_@babel+core@7.28.0_@babel+preset-env@7.28.0_@babel+core@7.28.0__@re_c4b2b877ff76cdef11524e8484232852/node_modules:/Users/harun.karahan/Mendix/native-widgets/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../react-native@0.77.3_@babel+core@7.28.0_@babel+preset-env@7.28.0_@babel+core@7.28.0__@re_c4b2b877ff76cdef11524e8484232852/node_modules/react-native/cli.js" "$@"
else
  exec node  "$basedir/../../../../../react-native@0.77.3_@babel+core@7.28.0_@babel+preset-env@7.28.0_@babel+core@7.28.0__@re_c4b2b877ff76cdef11524e8484232852/node_modules/react-native/cli.js" "$@"
fi
